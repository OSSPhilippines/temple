{
  "name": "@ossph/temple-language",
  "version": "0.0.13",
  "private": true,
  "description": "Webpack support for the Temple markup language.",
  "license": "Apache-2.0",
  "author": "Chris <chris@incept.asia>",
  "homepage": "https://github.com/OSSPhilippines/temple",
