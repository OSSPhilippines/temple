                <a 
                  class="block tx-center tx-white p-10 b-solid b-t-3 b-1" 
                  href="/temple/ui/form/slug.html"
                >
                  Slug
                </a>
              </div>
              <div class="basis-third-10 lg-basis-half-10 md-basis-full">
                <div class="bg-t-3 h-120 flex flex-center">
                  <field-switch 
                    name="switch" 
                    label="Active?" 
                    value="yes" 
