    "build:tsc": "tsc"
  },
  "devDependencies": {
    "@ossph/temple": "0.1.6",
    "@types/dompurify": "3.0.5",
    "@types/inputmask": "5.0.7",
    "@types/node": "22.1.0",
    "@types/prismjs": "1.26.4",
    "codemirror": "5.65.17",
    "dayjs": "1.11.13",
    "dompurify": "3.1.6",
    "inputmask": "5.0.9",
    "marked": "14.1.2",
    "prismjs": "1.29.0",
    "suneditor": "2.47.0",
    "typescript": "5.5.4"
  }
