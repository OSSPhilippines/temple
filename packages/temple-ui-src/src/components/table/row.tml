<script>
  const { col } = this.props;
  const children = this.originalChildren;
  this.classList.add('table-row');
  if (typeof col === 'string') {
    const classList = col.split(' ').filter(col => col.length > 0);
    if (classList.length > 0) {
      children.forEach(child => child.classList?.add(...classList));
    }
  }
</script>
{children}